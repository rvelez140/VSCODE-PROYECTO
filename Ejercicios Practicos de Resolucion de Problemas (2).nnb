            "source": [
                "let respuesta = confirm(\"¿Quieres continuar?\");\r\nif (respuesta) {\r\n    console.log(\"El usuario aceptó.\");\r\n} else {\r\n    console.log(\"El usuario canceló.\");\r\n}"
            ],
